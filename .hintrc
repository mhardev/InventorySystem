{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-filter",
          "backdrop-filter",
          "-moz-font-feature-settings",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "user-select",
          "mask-position",
          "mask-size",
          "mask-image",
          "print-color-adjust",
          "color: map-get($colors, color)"
        ]
      }
    ],
    "css-prefix-order": "off"
  }
}